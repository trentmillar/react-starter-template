{
  "name": "react-starter-template",
  "version": "1.0.0",
  "description": "Reactjs starter kit",
  "main": "webpack.config.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "all": "run-s clean build serve",
    "build": "NODE_ENV=production IS_DEBUG=true node_modules/.bin/webpack -p",
    "clean": "rimraf src/public/js/bundle.js",
    "serve": "node_modules/.bin/http-server src/public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/trentmillar/react-starter-template.git"
  },
  "keywords": [
    "babel",
    "bootstrap",
    "es6",
    "express",
    "node",
    "pug",
    "react",
    "reactjs",
    "router",
    "starter",
    "template"
  ],
  "author": "Trent Millar",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "express": "^4.14.0",
    "pug": "^0.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.9",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  }
}
